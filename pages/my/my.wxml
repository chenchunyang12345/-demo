<!-- 底部setting -->
<view class="m-setting">
  <van-grid column-num="3" clickable>
    <block wx:for="{{settingList}}" wx:key="index">
      <van-grid-item 
        icon="{{item.icon}}" 
        text="{{item.text}}"
        url="{{item.url}}"
      />
    </block>
  </van-grid>
</view>

<!-- 轻提示 -->
<van-toast id="van-toast" />